{
    "name": "nikoleesg/laravel-admin",
    "description": "laravel admin",
    "version": "1.8.22",
    "type": "library",
    "keywords": ["laravel", "admin", "grid", "form"],
    "homepage": "https://github.com/z-song/laravel-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "zsong",
            "email": "zosong@126.com"
        },
        {
          "name": "nikoleesg",
          "email": "niko.lee@outlook.sg"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/dom-crawler": "^6.0|^7.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "nikoleesg/data-table": "^1.0",
        "spatie/eloquent-sortable": "^4.5",
        "doctrine/dbal": "^3.7 || ^4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "intervention/image": "^2.7 || ^3.0",
        "laravel/browser-kit-testing": "^7.0",
        "spatie/phpunit-watcher": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Models\\": "tests/models",
            "Tests\\Controllers\\": "tests/controllers"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "suggest": {
        "intervention/image": "Required to handling and manipulation upload images (~2.3).",
        "spatie/eloquent-sortable": "Required to built orderable gird."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Encore\\Admin\\Facades\\Admin"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
